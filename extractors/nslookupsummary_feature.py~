
from extractor import Extractor
class nslookupsummary(Extractor):
    def __init__(self, summary_str, **kwargs):
        print "nslookupsummary"
        self.summary = summary_str
        
    def extract(self):
        return [self.get_total_servers()]
    
    def get_total_server(self):
        return int(self.summary.split(' ')[-1])
